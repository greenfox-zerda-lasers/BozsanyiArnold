{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "modules": false,
    "rules": {
      "no-var": "off",
      "no-console": "off",
      "strict": "off",
      "vars-on-top": "off",
      "prefer-template": "off",
      "func-names": "off",
      "prefer-arrow-callback": "off",
      "comma-dangle": "off"
    }
}
